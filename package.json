  "description": "T8D Project Monorepo",
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "frontend:dev": "pnpm --filter t8d-frontend dev",
    "frontend:build": "pnpm --filter t8d-frontend build",
    "backend:dev": "pnpm --filter t8d-backend dev",
    "backend:build": "pnpm --filter t8d-backend build",
    "dev:all": "pnpm -r --parallel dev"
  },
